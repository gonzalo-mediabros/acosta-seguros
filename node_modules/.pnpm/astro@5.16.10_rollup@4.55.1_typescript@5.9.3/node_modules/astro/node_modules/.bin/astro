#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/s/Proyectos/01_Clientes/Mediabros/Acosta Seguros/node_modules/.pnpm/astro@5.16.10_rollup@4.55.1_typescript@5.9.3/node_modules/astro/node_modules:/mnt/s/Proyectos/01_Clientes/Mediabros/Acosta Seguros/node_modules/.pnpm/astro@5.16.10_rollup@4.55.1_typescript@5.9.3/node_modules:/mnt/s/Proyectos/01_Clientes/Mediabros/Acosta Seguros/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/s/Proyectos/01_Clientes/Mediabros/Acosta Seguros/node_modules/.pnpm/astro@5.16.10_rollup@4.55.1_typescript@5.9.3/node_modules/astro/node_modules:/mnt/s/Proyectos/01_Clientes/Mediabros/Acosta Seguros/node_modules/.pnpm/astro@5.16.10_rollup@4.55.1_typescript@5.9.3/node_modules:/mnt/s/Proyectos/01_Clientes/Mediabros/Acosta Seguros/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../astro.js" "$@"
else
  exec node  "$basedir/../../astro.js" "$@"
fi
